    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],

            "env": {
                "MONGO_URI": "mongodb://127.0.0.1:27017/rosterize",
                "PORT": "5000",
                "TOKEN_SECRET": "secret",
                "ADMIN_EMAIL": "admin@gmail.com",
                "ADMIN_PASSWORD": "admin",
                "FRONTEND_URL": "http://localhost:5173"
            },
            "program": "${workspaceFolder}\\server\\routes\\Company.js"
        }
    ]
}